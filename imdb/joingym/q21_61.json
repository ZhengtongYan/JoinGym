 "query": "SELECT MIN(cn.name) AS company_name, MIN(lt.link) AS link_type, MIN(t.title) AS german_follow_up FROM company_name AS cn, company_type AS ct, keyword AS k, link_type AS lt, movie_companies AS mc, movie_info AS mi, movie_keyword AS mk, movie_link AS ml, title AS t WHERE lt.id = ml.link_type_id AND ml.movie_id = t.id AND t.id = mk.movie_id AND mk.keyword_id = k.id AND t.id = mc.movie_id AND mc.company_type_id = ct.id AND mc.company_id = cn.id AND mi.movie_id = t.id AND ml.movie_id = mk.movie_id AND ml.movie_id = mc.movie_id AND mk.movie_id = mc.movie_id AND ml.movie_id = mi.movie_id AND mk.movie_id = mi.movie_id AND mc.movie_id = mi.movie_id AND (k.keyword like '%drama%' OR k.keyword like '%betrayal%') AND t.production_year < 2011 AND (cn.name like '%Warner Bros. Pictures%' OR cn.name like '%Universal Pictures%') AND (mc.note like '%India%' OR mc.note like '%Mexico%' OR mc.note like '%USA%' OR mc.note like '%Portugal%' OR mc.note like '%Japan%') AND lt.link in ('spoofed in','follows','edited from','alternate language version of','featured in','similar to','spoofs','references','features','version of','spin off','unknown link','followed by','spin off from','remade as') AND ct.kind in ('production companies','special effects companies','miscellaneous companies') AND mi.info in ('Kalmyk-Oirat','Arabic','Albanian','Tagalog')",
 "join columns": ["mc.company_type_id", "mc.movie_id", "mc.company_id", "k.id", "lt.id", "ml.movie_id", "mk.keyword_id", "mi.movie_id", "ml.link_type_id", "mk.movie_id", "ct.id", "t.id", "cn.id"],
