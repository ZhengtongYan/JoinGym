 "query": "SELECT MIN(cn.name) AS company_name, MIN(lt.link) AS link_type, MIN(t.title) AS german_follow_up FROM company_name AS cn, company_type AS ct, keyword AS k, link_type AS lt, movie_companies AS mc, movie_info AS mi, movie_keyword AS mk, movie_link AS ml, title AS t WHERE lt.id = ml.link_type_id AND ml.movie_id = t.id AND t.id = mk.movie_id AND mk.keyword_id = k.id AND t.id = mc.movie_id AND mc.company_type_id = ct.id AND mc.company_id = cn.id AND mi.movie_id = t.id AND ml.movie_id = mk.movie_id AND ml.movie_id = mc.movie_id AND mk.movie_id = mc.movie_id AND ml.movie_id = mi.movie_id AND mk.movie_id = mi.movie_id AND mc.movie_id = mi.movie_id AND (k.keyword like '%comedy%' OR k.keyword like '%terror%' OR k.keyword like '%betrayal%' OR k.keyword like '%revolution%' OR k.keyword like '%future%') AND mi.info not in ('Portuguese','Arabic','Cantonese') AND mc.note not like '%Denmark%' AND mc.note not like '%Japan%' AND mc.note not like '%Italy%' AND mc.note not like '%France%' AND mc.note not like '%Germany%' AND cn.country_code not in ('an','kh','iq','sm','kz','pm','mg','ao','ng','ru') AND (cn.name like '%Miramax%' OR cn.name like '%DreamWorks Animation%' OR cn.name like '%MGM Studios%' OR cn.name like '%Blumhouse Productions%') AND ct.kind in ('production companies','miscellaneous companies','special effects companies')",
 "join columns": ["mc.company_type_id", "mc.movie_id", "mc.company_id", "k.id", "lt.id", "ml.movie_id", "mk.keyword_id", "mi.movie_id", "ml.link_type_id", "mk.movie_id", "ct.id", "t.id", "cn.id"],
 "unary columns": ["cn.country_code", "mc.note", "k.keyword", "cn.name", "ct.kind", "mi.info"],
