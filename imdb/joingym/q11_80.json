 "query": "SELECT MIN(cn.name) AS from_company, MIN(lt.link) AS movie_link_type, MIN(t.title) AS non_polish_sequel_movie FROM company_name AS cn, company_type AS ct, keyword AS k, link_type AS lt, movie_companies AS mc, movie_keyword AS mk, movie_link AS ml, title AS t WHERE lt.id = ml.link_type_id AND ml.movie_id = t.id AND t.id = mk.movie_id AND mk.keyword_id = k.id AND t.id = mc.movie_id AND mc.company_type_id = ct.id AND mc.company_id = cn.id AND ml.movie_id = mk.movie_id AND ml.movie_id = mc.movie_id AND mk.movie_id = mc.movie_id AND t.production_year < 2002 AND (t.title like '%island%' OR t.title like '%shield%' OR t.title like '%thunder%' OR t.title like '%dream%') AND lt.link in ('spoofed in','follows','similar to','followed by','spin off','spoofs','remake of','edited into','features','referenced in','version of','spin off from','edited from','unknown link','alternate language version of') AND cn.country_code not in ('bo','nz','cshh','id','si','au','sz') AND (cn.name like '%Amblin Television%' OR cn.name like '%Miramax%' OR cn.name like '%STX%' OR cn.name like '%Working Title Films%' OR cn.name like '%Universal Pictures%') AND ct.kind in ('production companies','special effects companies','miscellaneous companies') AND (k.keyword like '%drama%')",
 "join columns": ["cn.id", "t.id", "lt.id", "mk.keyword_id", "mc.movie_id", "ml.movie_id", "k.id", "mk.movie_id", "mc.company_id", "mc.company_type_id", "ct.id", "ml.link_type_id"],
 "unary columns": ["t.production_year", "lt.link", "k.keyword", "cn.name", "ct.kind", "t.title", "cn.country_code"],
 "join expressions": [{"left": "lt.id", "right": "ml.link_type_id"}, {"left": "ml.movie_id", "right": "t.id"}, {"left": "t.id", "right": "mk.movie_id"}, {"left": "mk.keyword_id", "right": "k.id"}, {"left": "t.id", "right": "mc.movie_id"}, {"left": "mc.company_type_id", "right": "ct.id"}, {"left": "mc.company_id", "right": "cn.id"}, {"left": "ml.movie_id", "right": "mk.movie_id"}, {"left": "ml.movie_id", "right": "mc.movie_id"}, {"left": "mk.movie_id", "right": "mc.movie_id"}],
 "left deep tree min cost": "0",
 "left deep tree min order": "(((((((ml join t) join cn) join ct) join k) join lt) join mc) join mk)",
