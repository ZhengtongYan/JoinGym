 "query": "SELECT MIN(cn.name) AS from_company, MIN(lt.link) AS movie_link_type, MIN(t.title) AS non_polish_sequel_movie FROM company_name AS cn, company_type AS ct, keyword AS k, link_type AS lt, movie_companies AS mc, movie_keyword AS mk, movie_link AS ml, title AS t WHERE lt.id = ml.link_type_id AND ml.movie_id = t.id AND t.id = mk.movie_id AND mk.keyword_id = k.id AND t.id = mc.movie_id AND mc.company_type_id = ct.id AND mc.company_id = cn.id AND ml.movie_id = mk.movie_id AND ml.movie_id = mc.movie_id AND mk.movie_id = mc.movie_id AND lt.link in ('version of','followed by','remade as','spin off','features','spoofs','edited from','featured in') AND t.production_year < 2007 AND (t.title like '%book%' OR t.title like '%heart%' OR t.title like '%mirror%') AND ct.kind not in ('production companies','distributors')",
 "join columns": ["cn.id", "t.id", "lt.id", "mk.keyword_id", "mc.movie_id", "ml.movie_id", "k.id", "mk.movie_id", "mc.company_id", "mc.company_type_id", "ct.id", "ml.link_type_id"],
 "unary columns": ["t.production_year", "ct.kind", "lt.link", "t.title"],
 "join expressions": [{"left": "lt.id", "right": "ml.link_type_id"}, {"left": "ml.movie_id", "right": "t.id"}, {"left": "t.id", "right": "mk.movie_id"}, {"left": "mk.keyword_id", "right": "k.id"}, {"left": "t.id", "right": "mc.movie_id"}, {"left": "mc.company_type_id", "right": "ct.id"}, {"left": "mc.company_id", "right": "cn.id"}, {"left": "ml.movie_id", "right": "mk.movie_id"}, {"left": "ml.movie_id", "right": "mc.movie_id"}, {"left": "mk.movie_id", "right": "mc.movie_id"}],
 "left deep tree min cost": "0",
 "left deep tree min order": "(((((((ct join mc) join cn) join k) join lt) join mk) join ml) join t)",
 "bushy deep tree min cost": "0",
 "bushy deep tree min order": "(((((((ct join mc) join cn) join k) join lt) join mk) join ml) join t)"
