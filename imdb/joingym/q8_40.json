 "query": "SELECT MIN(an.name) AS acress_pseudonym, MIN(t.title) AS japanese_anime_movie FROM aka_name AS an, cast_info AS ci, company_name AS cn, movie_companies AS mc, name AS n, role_type AS rt, title AS t WHERE an.person_id = n.id AND n.id = ci.person_id AND ci.movie_id = t.id AND t.id = mc.movie_id AND mc.company_id = cn.id AND ci.role_id = rt.id AND an.person_id = ci.person_id AND ci.movie_id = mc.movie_id AND t.production_year > 1994 AND (t.title like '%ship%' OR t.title like '%dress%' OR t.title like '%drama%' OR t.title like '%road%' OR t.title like '%violin%') AND (cn.name like '%Legendary%' OR cn.name like '%Pixar Animation Studios%' OR cn.name like '%Universal Pictures%' OR cn.name like '%Lionsgate%')",
 "join columns": ["n.id", "cn.id", "ci.person_id", "an.person_id", "rt.id", "t.id", "ci.role_id", "mc.movie_id", "mc.company_id", "ci.movie_id"],
