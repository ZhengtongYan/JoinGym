 "query": "SELECT MIN(an.name) AS acress_pseudonym, MIN(t.title) AS japanese_anime_movie FROM aka_name AS an, cast_info AS ci, company_name AS cn, movie_companies AS mc, name AS n, role_type AS rt, title AS t WHERE an.person_id = n.id AND n.id = ci.person_id AND ci.movie_id = t.id AND t.id = mc.movie_id AND mc.company_id = cn.id AND ci.role_id = rt.id AND an.person_id = ci.person_id AND ci.movie_id = mc.movie_id AND n.gender in ('m') AND (n.name like '%k%' OR n.name like '%a%' OR n.name like '%Y%' OR n.name like '%t%') AND t.production_year > 2000 AND (t.title like '%wand%' OR t.title like '%spoon%' OR t.title like '%picture%' OR t.title like '%drama%' OR t.title like '%mirror%') AND (mc.note like '%Mexico%' OR mc.note like '%India%' OR mc.note like '%Denmark%' OR mc.note like '%Sweden%' OR mc.note like '%Portugal%')",
 "join columns": ["n.id", "cn.id", "ci.person_id", "an.person_id", "rt.id", "t.id", "ci.role_id", "mc.movie_id", "mc.company_id", "ci.movie_id"],
