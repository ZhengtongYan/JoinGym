 "query": "SELECT MIN(cn.name) AS company_name, MIN(lt.link) AS link_type, MIN(t.title) AS german_follow_up FROM company_name AS cn, company_type AS ct, keyword AS k, link_type AS lt, movie_companies AS mc, movie_info AS mi, movie_keyword AS mk, movie_link AS ml, title AS t WHERE lt.id = ml.link_type_id AND ml.movie_id = t.id AND t.id = mk.movie_id AND mk.keyword_id = k.id AND t.id = mc.movie_id AND mc.company_type_id = ct.id AND mc.company_id = cn.id AND mi.movie_id = t.id AND ml.movie_id = mk.movie_id AND ml.movie_id = mc.movie_id AND mk.movie_id = mc.movie_id AND ml.movie_id = mi.movie_id AND mk.movie_id = mi.movie_id AND mc.movie_id = mi.movie_id AND ct.kind in ('production companies','special effects companies','distributors') AND lt.link in ('spin off from','unknown link','referenced in','featured in') AND t.production_year > 1992 AND (t.title like '%potion%' OR t.title like '%wind%') AND (k.keyword like '%adventure%' OR k.keyword like '%drama%' OR k.keyword like '%sacrifice%' OR k.keyword like '%fantasy%')",
 "join columns": ["mc.company_type_id", "mc.movie_id", "mc.company_id", "k.id", "lt.id", "ml.movie_id", "mk.keyword_id", "mi.movie_id", "ml.link_type_id", "mk.movie_id", "ct.id", "t.id", "cn.id"],
