 "query": "SELECT MIN(n.name) AS of_person, MIN(t.title) AS biography_movie FROM aka_name AS an, cast_info AS ci, info_type AS it, link_type AS lt, movie_link AS ml, name AS n, person_info AS pi, title AS t WHERE n.id = an.person_id AND n.id = pi.person_id AND ci.person_id = n.id AND t.id = ci.movie_id AND ml.linked_movie_id = t.id AND lt.id = ml.link_type_id AND it.id = pi.info_type_id AND pi.person_id = an.person_id AND pi.person_id = ci.person_id AND an.person_id = ci.person_id AND ci.movie_id = ml.linked_movie_id AND (an.name like '%B%' OR an.name like '%C%' OR an.name like '%O%' OR an.name like '%Y%') AND lt.link in ('references','follows','unknown link','referenced in','similar to','features','spin off','remake of','edited into','edited from','remade as','alternate language version of','version of','followed by','featured in') AND (pi.note like '%BEF%') AND it.info in ('filming dates','sound mix','weekend gross','LD length','portrayed in','soundtrack','LD laserdisc title','death notes','opening weekend','LD spaciality','LD certification') AND n.gender in ('m') AND t.production_year > 1995 AND (t.title like '%suit%' OR t.title like '%hat%')",
 "join columns": ["n.id", "ml.linked_movie_id", "ci.person_id", "an.person_id", "pi.person_id", "t.id", "lt.id", "it.id", "pi.info_type_id", "ci.movie_id", "ml.link_type_id"],
 "unary columns": ["t.production_year", "an.name", "lt.link", "it.info", "t.title", "n.gender", "pi.note"],
 "join expressions": [{"left": "n.id", "right": "an.person_id"}, {"left": "n.id", "right": "pi.person_id"}, {"left": "ci.person_id", "right": "n.id"}, {"left": "t.id", "right": "ci.movie_id"}, {"left": "ml.linked_movie_id", "right": "t.id"}, {"left": "lt.id", "right": "ml.link_type_id"}, {"left": "it.id", "right": "pi.info_type_id"}, {"left": "pi.person_id", "right": "an.person_id"}, {"left": "pi.person_id", "right": "ci.person_id"}, {"left": "an.person_id", "right": "ci.person_id"}, {"left": "ci.movie_id", "right": "ml.linked_movie_id"}],
