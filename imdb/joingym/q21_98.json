 "query": "SELECT MIN(cn.name) AS company_name, MIN(lt.link) AS link_type, MIN(t.title) AS german_follow_up FROM company_name AS cn, company_type AS ct, keyword AS k, link_type AS lt, movie_companies AS mc, movie_info AS mi, movie_keyword AS mk, movie_link AS ml, title AS t WHERE lt.id = ml.link_type_id AND ml.movie_id = t.id AND t.id = mk.movie_id AND mk.keyword_id = k.id AND t.id = mc.movie_id AND mc.company_type_id = ct.id AND mc.company_id = cn.id AND mi.movie_id = t.id AND ml.movie_id = mk.movie_id AND ml.movie_id = mc.movie_id AND mk.movie_id = mc.movie_id AND ml.movie_id = mi.movie_id AND mk.movie_id = mi.movie_id AND mc.movie_id = mi.movie_id AND lt.link in ('remade as','spoofs','spin off','similar to','alternate language version of','follows','featured in','edited from','version of','followed by','references','edited into') AND (cn.name like '%Working Title Films%' OR cn.name like '%A24%' OR cn.name like '%Walt Disney Studios%' OR cn.name like '%Lionsgate%') AND cn.country_code not in ('je','ma','gi','np','lv','my','sj','zm') AND ct.kind in ('special effects companies','distributors','miscellaneous companies') AND (t.title like '%camera%' OR t.title like '%secret%' OR t.title like '%game%' OR t.title like '%treasure%') AND t.production_year < 2002",
 "join columns": ["mc.company_type_id", "mc.movie_id", "mc.company_id", "k.id", "lt.id", "ml.movie_id", "mk.keyword_id", "mi.movie_id", "ml.link_type_id", "mk.movie_id", "ct.id", "t.id", "cn.id"],
 "unary columns": ["cn.country_code", "cn.name", "t.title", "lt.link", "ct.kind", "t.production_year"],
 "join expressions": [{"left": "lt.id", "right": "ml.link_type_id"}, {"left": "ml.movie_id", "right": "t.id"}, {"left": "t.id", "right": "mk.movie_id"}, {"left": "mk.keyword_id", "right": "k.id"}, {"left": "t.id", "right": "mc.movie_id"}, {"left": "mc.company_type_id", "right": "ct.id"}, {"left": "mc.company_id", "right": "cn.id"}, {"left": "mi.movie_id", "right": "t.id"}, {"left": "ml.movie_id", "right": "mk.movie_id"}, {"left": "ml.movie_id", "right": "mc.movie_id"}, {"left": "mk.movie_id", "right": "mc.movie_id"}, {"left": "ml.movie_id", "right": "mi.movie_id"}, {"left": "mk.movie_id", "right": "mi.movie_id"}, {"left": "mc.movie_id", "right": "mi.movie_id"}],
 "left deep tree min cost": "0",
